{
  "version": 2,
  "env": {
    "REACT_APP_OPENROUTER_API_KEY": "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"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}